    "version": "1.0.0",
    "description": "Supermarket DevOps Project",
    "scripts": {
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
      "lint:css": "stylelint \"**/*.css\"",
      "lint:css:fix": "stylelint \"**/*.css\" --fix",
      "precommit": "npm run lint && npm run lint:css",
