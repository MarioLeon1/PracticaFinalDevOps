      "build:dev": "webpack --mode development",
      "docker:build": "docker-compose build",
      "docker:build:prod": "docker-compose -f docker-compose.prod.yml build"
    },
    "devDependencies": {
      "eslint": "^8.55.0",
      "eslint-plugin-node": "^11.1.0",
      "prettier": "^3.1.0",
      "stylelint": "^15.11.0",
